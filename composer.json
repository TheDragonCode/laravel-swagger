{
    "name": "andrey-helldar/laravel-swagger",
    "description": "Documentation generation for Swagger without OpenAPI",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "laravel",
        "swagger",
        "json",
        "yaml",
        "rest",
        "api",
        "doc",
        "documentation"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@dragon-code.pro",
            "homepage": "https://github.com/andrey-helldar"
        }
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/laravel-swagger/issues",
        "source": "https://github.com/andrey-helldar/laravel-swagger"
    },
    "require": {
        "php": "^7.2.5 || ^8.0",
        "ext-json": "*",
        "ext-yaml": "*",
        "andrey-helldar/laravel-routes-core": "^2.0",
        "andrey-helldar/support": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "illuminate/console": "^7.0 || ^8.0",
        "illuminate/http": "^7.0 || ^8.0",
        "illuminate/routing": "^7.0 || ^8.0",
        "illuminate/support": "^7.0 || ^8.0",
        "phpdocumentor/reflection-docblock": "^5.1",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^5.0 || ^6.0",
        "phpunit/phpunit": "^9.4"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Helldar\\LaravelSwagger\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Helldar\\LaravelSwagger\\ServiceProvider"
            ]
        },
        "thanks": [
            {
                "name": "andrey-helldar/laravel-routes-core",
                "url": "https://github.com/andrey-helldar/laravel-routes-core"
            },
            {
                "name": "andrey-helldar/support",
                "url": "https://github.com/andrey-helldar/support"
            }
        ]
    }
}