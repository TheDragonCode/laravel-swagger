{
    "name": "andrey-helldar/laravel-swagger",
    "description": "Documentation generation for Swagger without OpenAPI",
    "type": "library",
    "license": "MIT",
    "keywords": ["api", "laravel", "swagger"],
    "support": {
        "issues": "https://github.com/andrey-helldar/swagger/issues",
        "source": "https://github.com/andrey-helldar/swagger"
    },
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/console": "^7.0",
        "illuminate/routing": "^7.0",
        "illuminate/support": "^7.0"
    },
    "autoload": {
        "classmap": [
            "src"
        ],
        "psr-4": {
            "Helldar\\LaravelSwagger\\": "src"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
